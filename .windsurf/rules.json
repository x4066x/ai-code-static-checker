{
  "version": "1.0",
  "rules": {
    "symbolAnalysis": {
      "checkBeforeModification": true,
      "detectUnusedSymbols": true,
      "validateDependencies": true
    },
    "fileAnalysis": {
      "detectCircularDependencies": true,
      "validateImports": true,
      "checkDeadCode": true
    },
    "preModificationChecks": {
      "analyzeSymbolReferences": true,
      "validateFileStructure": true,
      "checkDependencyGraph": true
    },
    "postModificationChecks": {
      "validateNewReferences": true,
      "checkUnreferencedSymbols": true,
      "detectCircularDeps": true
    }
  }
}
